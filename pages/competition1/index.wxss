.container {
  background-color: #0A185A;
  display: flex;
  justify-content: space-between;
  color: white;
  height: 100%;
}
.competation-detail-img image{
  width: 145px;
  height: 152px;
  border: 2px solid #FFFFFF;
  opacity: 1;
  border-radius: 10px 10px 0px 0px;
  background-repeat: no-repeat;
}
.title {
  width: 128px;
  height: 20px;
  font-size: 16px;
  font-family: Microsoft YaHei UI;
  font-weight: bold;
  color: #1B1B1B;
  opacity: 1;
}
.date {
  height: 10px;
  font-size: 8px;
  font-family: Microsoft YaHei UI;
  font-weight: 400;
  color: #1B1B1B;
  opacity: 1;
}
.price {
  width: 39px;
  height: 15px;
  font-size: 12px;
  font-family: Microsoft YaHei UI;
  font-weight: bold;
  color: #0024CD;
  opacity: 1;
}
.address {
  width: 119px;
  height: 10px;
  font-size: 8px;
  font-family: Microsoft YaHei UI;
  font-weight: 400;
  color: #7E7E7E;
  opacity: 1;
}
.competation-list-detail {
  width: 152px;
  height: 246px;
  background: #FFFFFF;
  opacity: 1;
  border-radius: 10px;
}
.competation-detail-img {
  height: 152px;
}
.competation-detail-desc {
  margin-left: 9px;
  padding-top: 5px;
}
.competation-detail-desc view{
  padding-top: 5px;
}
.competation-list {
  display: flex;
  justify-content: space-around;
}
.competation-detail-img .status {
  background-repeat: no-repeat;
  height: 20px;
  background-size: 40px 14px;
  font-size: 8px;
  line-height: 14px;
  padding-left: 8px;
}
.competation-detail-img {
  padding-top: 8px;
  padding-left: 6px;
}

.foot {
  width: 345px;
  height: 70px;
  background-color: #00082C;
}

.foot .main {
  width: 173px;
  height: 70px;
  background-image: url("data:image/png;base64,");

}
.foot-content {
  line-height: 70px;
  width: 173px;
  text-align: center;
}

.foot {
  display: flex;
  border-radius: 50px;
  display: flex;
  border-radius: 51px;
  position: absolute;
  left: 13px;
  bottom: 15px;
}